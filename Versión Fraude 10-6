        "# Usuario Origen\n",
        "top_origen = (\n",
        "    df_mod_col[df_mod_col['isFraud'] == 1]\n",
        "    .groupby('nameOrig')\n",
        "    .agg(\n",
        "        cantidad_fraudes=('isFraud', 'count'),\n",
        "        monto_total=('amount', 'sum')\n",
        "    )\n",
        "    .sort_values(by='monto_total', ascending=False)\n",
        "\n",
